{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "security/no-low-level-calls": 0,
    "security/no-block-members": [0, ["timestamp"]],
    "operator-whitespace": 0,
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "error",
      4
    ]
  }
}
